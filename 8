numero = int(input("Introduce un número: "))# Solicitar al usuario que introduzca un número
print(f"Tabla de multiplicar del {numero}:")#dice de que numero es la tabla de multiplicar
for i in range(1, 11):#se repite mientras "i" este en rango de 1 a 11
    print(f"{numero} x {i} = {numero * i}")#coloca y realiza la tabla de multiplicar
